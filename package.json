{
  "name": "onco-assist-tidb",
  "version": "1.0.0",
  "description": "AI-powered medical assistant with TiDB vector database integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js start",
    "server": "node dist/index.js server",
    "dev": "ts-node src/index.ts start",
    "dev:server": "ts-node src/index.ts server",
    "watch": "ts-node --watch src/index.ts start",
    "process": "node dist/index.js process",
    "report": "node dist/index.js report",
    "search": "node dist/index.js search",
    "setup": "node scripts/setup.js",
    "auth:google": "node scripts/google-auth-setup.js",
    "test:setup": "node scripts/test-setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t onco-assist .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "medical",
    "ai",
    "tidb",
    "vector-database",
    "hackathon",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/documentai": "^8.8.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "googleapis": "^133.0.0",
    "helmet": "^8.1.0",
    "mysql2": "^3.9.1",
    "node-cron": "^3.0.3",
    "openai": "^4.26.0",
    "twilio": "^4.20.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
